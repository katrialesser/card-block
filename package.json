{
  "name": "card-row",
  "version": "0.0.1",
  "description": "A custom block for Gutenberg Cloud",
  "main": "build/index.js",
  "gutenbergCloud": {
    "js": "build/index.js",
    "css": "build/style.css",
    "screenshot": "screenshot.png",
    "name": "My Custom Block"
  },
  "scripts": {
    "build": "set NODE_ENV=production webpack",
    "start": "npm run build && npm --prefix node_modules/@frontkom/g-editor start",
    "deploy": "npm run build && npm publish --access public"
  },
  "keywords": [
    "gutenberg",
    "gutenberg-cloud"
  ],
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.6.0"
  },
  "peerDependencies": {
    "react": "^16.3.1"
  },
  "devDependencies": {
    "@frontkom/g-editor": "^0.3.4",
    "@frontkom/gutenberg-js": "^3.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.5",
    "babel-preset-react-app": "^3.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "react": "^16.3.1",
    "sass-loader": "^7.1.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "files": [
    "build/",
    "screenshot.png"
  ],
  "dependencies": {
    "react": "^16.6.3",
    "tinymce": "^4.9.1"
  }
}
